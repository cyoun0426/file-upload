<!--  !DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent"-->
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:Label>Here you select a file to upload</g:Label>
		<g:FormPanel ui:field="form">
			<g:HTMLPanel>
				<g:FileUpload ui:field="fileSelector" name="uploadForm"></g:FileUpload>
				<g:Button ui:field="uploadButton">Upload</g:Button>
			</g:HTMLPanel>
		</g:FormPanel>
	</g:HTMLPanel>
</ui:UiBinder> 